<h1>Listing ctecs</h1>

<table>
  <thead>
    <tr>
      <th>Course id</th>
      <th>Description</th>
      <th>Instr rating</th>
      <th>Course rating</th>
      <th>Learned rating</th>
      <th>Challenging rating</th>
      <th>Interest rating</th>
      <th>School</th>
      <th>Class</th>
      <th>Reason</th>
      <th>Interest before</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @ctecs.each do |ctec| %>
      <tr>
        <td><%= ctec.course_ID %></td>
        <td><%= ctec.description %></td>
        <td><%= ctec.instr_rating %></td>
        <td><%= ctec.course_rating %></td>
        <td><%= ctec.learned_rating %></td>
        <td><%= ctec.challenging_rating %></td>
        <td><%= ctec.interest_rating %></td>
        <td><%= ctec.school %></td>
        <td><%= ctec.class %></td>
        <td><%= ctec.reason %></td>
        <td><%= ctec.interest_before %></td>
        <td><%= link_to 'Show', ctec %></td>
        <td><%= link_to 'Edit', edit_ctec_path(ctec) %></td>
        <td><%= link_to 'Destroy', ctec, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ctec', new_ctec_path %>
